<div id="left" class="editor grid_6 alpha">
    <textarea id="template" name="template" class="watermarkOn" rows="20" cols="80"></textarea>
</div>
<div id="right" class="grid_6 omega">
    <div id="model">
        <div class="tModel visual_search">
            &nbsp;
        </div>
        <table id="modelProps">
            <thead>
                <tr>
                    <th>
                        Name
                    </th>
                    <th>
                        Type
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        Prop1
                    </td>
                    <td>
                        int
                    </td>
                </tr>
                <tr>
                    <td>
                        Prop2
                    </td>
                    <td>
                        bool
                    </td>
                </tr>
                <tr>
                    <td>
                        Prop3
                    </td>
                    <td>
                        string
                    </td>
                </tr>
                <tr>
                    <td>
                        &nbsp;
                    </td>
                    <td>
                        &nbsp;
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
<div class="results grid_12">
    <div id="resultTabs" class="tabs results">
        <ul>
            <li><a href="#rendered-output-container">Rendered Output</a></li>
            <li><a href="#template-output-container">Template Output</a></li>
            <li><a href="#parser-result-container">Parser Results</a></li>
            <li><a href="#generated-code-container">Generated Code</a></li>
            <li><a href="#messages-container">Messages</a></li>
        </ul>
        <div id="rendered-output-container">
            <div id="rendered-output">
            </div>
        </div>
        <div id="template-output-container">
            <pre id="template-output" class="prettyprint"></pre>
        </div>
        <div id="parser-result-container">
        </div>
        <div id="generated-code-container">
        </div>
        <div id="messages-container">
            <ul id="messages">
            </ul>
        </div>
    </div>
</div>
<script type="text/javascript" src="@Url.Content("~/Scripts/RazorPad.js")"></script>
<script type="text/javascript" charset="utf-8">
    $(function () {
        $('#resultTabs').tabs();
        VS.init({
            container: $('.visual_search'),
            query: '',
            callbacks: {
                search: function (query) { },
                facetMatches: function (callback) {
                    callback([
                            'bool', 'byte', 'sbyte', 'char', 'decimal', 'double', 'float', 'int', 'uint', 'long', 'ulong', 'object', 'short', 'ushort', 'string'
                           ]);

                },
                valueMatches: function (facet, searchTerm, callback) { }
            }
        });

    });
</script>
